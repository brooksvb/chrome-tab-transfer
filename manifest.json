{
	"manifest_version": 2,
	"name": "Tab Transfer",
	"version": "0.1",
	
	"browser_action": {
    "default_icon": "icon.png",
	},
	
	"permissions": [
		"tabs"
	],
	
	"content_scripts": [
    {
		"matches": [
			"<all_urls>"
		],
		"js": ["content.js"]
	}
	],
	
	"background": {
		"scripts": ["background.js"]
	}
	
	
}