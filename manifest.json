{
  "manifest_version": 2,
  "name": "Tab Transfer",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js"]
    }
  ],
  "permissions": [
	"tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}